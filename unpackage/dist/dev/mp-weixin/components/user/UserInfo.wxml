<view class="user-info"><block wx:if="{{infoData.image}}"><view class="user-logo"><image src="{{infoData.image}}" mode="widthFix"></image></view></block><block wx:else><view class="user-logo"><image src="/static/image/empty-tx.png" mode="widthFix"></image></view></block><block wx:if="{{isLogin}}"><view class="user-name"><block wx:if="{{infoData.nickname}}"><view>{{infoData.nickname}}</view></block><block wx:else><view>还未设置昵称</view></block><view>{{infoData.telphone}}</view></view></block><block wx:if="{{isLogin===false}}"><view data-event-opts="{{[['tap',[['router',['/pages/user/user',2]]]]]}}" class="go-login" bindtap="__e">登录/注册</view></block><view data-event-opts="{{[['tap',[['router',['/pages/setting/setting']]]]]}}" class="user-erweima" bindtap="__e"><view class="iconfont icon-xiangyou"></view></view></view>