
.user-shop{
    height: 176rpx;
    margin-top: 20rpx;
    border-bottom:  10px solid #f5f5f5;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.shop-type{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
}
.shop-type>view:nth-child(1) image{
    height: 56rpx;
    width: 70rpx;
}
.shop-type>view:nth-child(2){
    height: 80rpx;
    line-height: 80rpx;
}

