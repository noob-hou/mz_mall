    height: 70rpx;
    width: 100%;
    background-color: #f7f7f7;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.serve>view{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
