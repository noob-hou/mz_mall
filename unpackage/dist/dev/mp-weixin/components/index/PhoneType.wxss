
.phone-type{
    width: 100%;
    height: 450rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding-top: 50rpx;
    border-bottom: 40rpx solid #f4f4f4;
    -webkit-justify-content:space-around;
            justify-content:space-around;
}
.phone-type-item{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    text-align: center;
}
.phone-type-img{
    width: 200rpx;
    height: 200rpx;
}
.phone-type-title{
    font-size: 24rpx;
    line-height: 150rpx;
    height: 150rpx;
    color: #000;
}

