<view class="login"><view class="login-logo"><image src="../../static/logo2.jpg" mode="widthFix"></image></view><view class="tel"><input type="tel" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','telphone','$event',[]]]]]]}}" value="{{telphone}}" bindinput="__e"/></view><view class="tel"><input type="text" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><view class="getCode"><button type="default" disabled="{{isDisabled}}" data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" bindtap="__e">{{codeTxt}}</button></view></view><view class="tel"><input type="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="button"><button type="primary" data-event-opts="{{[['tap',[['registerClick',['$event']]]]]}}" bindtap="__e">注册</button></view><view class="register"><view>已有账号 ？ |</view><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" bindtap="__e">点击去登录</view></view></view>